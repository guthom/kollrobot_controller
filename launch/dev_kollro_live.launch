<launch>  
  <!-- load params-->
  <arg name="limited" default="true"/>
  <arg name="robot_name" default="ur10"/>
  <arg name="robot_description" default="$(arg robot_name)/robot_description"/>

  <!-- send robot urdf to param server -->
   <include file="$(find kollrobot_description)/launch/upload.launch">
    <arg name="limited" value="$(arg limited)"/>
  </include>

  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot" respawn="false"/>

  <include file="$(find ur_gazebo)/launch/controller_utils.launch"/>

  <rosparam file="$(find ropose)/yaml/arm_controller_ur10.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller" respawn="false"/>
  
  <!-- upload robot descripton-->
  <include file="$(find kollrobot_moveit_config)/launch/kollrobot_moveit_planning_execution.launch">
    <arg name="sim" value="true"/>
  </include>

  <arg name="rviz_cfg" default="$(find ropose)/rvizConfig/demo_ur10_only.rviz" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rviz_cfg)"/>	


  <!-- run static information core node-->  
  <node name="static_information" pkg="kollro_solver" type="StaticInformation"/>	

 <!-- launch realsense-->
  <arg name="width" default="1280"/>
  <arg name="height" default="720"/>
    <node pkg="tf2_ros" type="static_transform_publisher" name="camera_link_tf" args="0 0 0 0 0 0 1 /world /camera_link"/>    
    <include file="$(find realsense2_camera)/launch/rs_rgbd.launch">
        <arg name="camera"            value="depthcam1"/>
        <arg name="serial_no"           value=""/>
        <arg name="json_file_path"      value="/home/thomas/ROS/RealSensCfg/highaccuracy.json"/>     
        <arg name="depth_width"       value="$(arg width)"/>
        <arg name="depth_height"      value="$(arg height)"/>
        <arg name="infra1_width"      value="$(arg width)"/>
        <arg name="infra1_height"     value="$(arg height)"/>
        <arg name="infra2_width"      value="$(arg width)"/>
        <arg name="infra2_height"     value="$(arg height)"/>
        <arg name="color_width"       value="$(arg width)"/>
        <arg name="color_height"      value="$(arg height)"/>
    </include>


  <include file="$(find qrcode_referencer)/launch/qrcode_referencer.launch">
  </include>
  
  </launch>
