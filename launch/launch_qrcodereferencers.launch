    <node pkg="tf2_ros" type="static_transform_publisher" name="camera_link_tf0" args="0.00 -0.01 0.04 -0.03 0.00 0.06 /$(arg cameraName)_link_temp2 /$(arg cameraName)_link"/>    
  
